.page{
  background: #f8f8f8;
  padding:1.1764em 15px 65px;
  min-height: 100%;
  box-sizing: border-box;
}
.weui-cells:before{
  content:none;
}
.weui-cells:after{
  content: none;
}
.weui-cell:before{
  content: none;
}
.myweui-cell__bd{
  font-size: 18px;
  color: #666;
}
.weui-cell__hd{
  font-size: 14px;
  color: #666;
  margin-right: 10px;
  width: 45px;
}
 .weui-cells{
  border: 1px solid #e4e4e4;
  /* border-radius: 5px; */
  overflow: hidden;
}

.weui-cells_after-title{
  margin-top: 20px;
}
.mybtn{
  height: 44px;
  background: white;
  border-top: 1px solid #e4e4e4;
  color: #1AAD19;
  border-radius: 0;
}
.mybtn:after{
  content: none;
}

.btm {
  background:white;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  padding: 10px;
  border-top: 1rpx solid #d9d9d9;
  box-sizing: border-box;
}

.btm button {
  padding: 0;
  width: 80px;
  font-size: 14px;
  background: white;
  color: #f60;
  border: 1px solid #f60;
  position: absolute;
  right: 10px;
  top: 5px;
}
.btm button:after{
  content: none;
}
