

.page {
  min-height: 100%;
}

.weui-cells__title {
  margin: 0;
  padding-top: 10px;
  padding-bottom: 10px;
}

.weui-cell:before {
  left: 0;
}

.weui-cell{
  height: 80px;
  box-sizing: border-box;
}
.title{
  font-size: 17px;
  color: #2b2d3a;
}
.part {
  margin-right: 20px;
  vertical-align: middle;
  width: 40px;
  height: 40px;
}

.part-text {
  font-size: 12px;
  color: #626574;
  margin-top: 10px;
}
