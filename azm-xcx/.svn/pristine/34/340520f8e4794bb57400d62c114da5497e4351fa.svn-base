/* pages/home2/home2.wxss */
/* register.wxss */
/* home.wxss */
.page{
  background: #f8f8f8;
  min-height: 100%;
}
.top {
  background: rgba(43, 45, 58, 0.6);
  color: white;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  padding: 10px;
}

.top button {
  padding: 0;
  width: 70px;
  float: right;
  margin-right: 20px;
}

.icons {
  /* border-top: 1px solid #ccc; */
  height: 80px;
  box-sizing: border-box;
  border-bottom: 1rpx solid #d9d9d9;
  padding: 10px 0;
  width: 100%;
  display: flex;
  flex-direction: row;
  background: white;
  margin-top: -9px;
}

.icon {
  width: 33.33%;
  text-align: center;
  font-size: 14px;
}

.icon image {
  width: 32px;
  height: 32px;
}
.weui-cells__title{
  padding: 10px 15px 5px;
}
.weui-cell:before{
  left: 0;
}
.weui-cell{
  height: 80px;
  box-sizing: border-box;
}
.title{
  font-size: 17px;
  color: #2b2d3a;
}
.part {
  margin-right: 20px;
  vertical-align: middle;
  width: 40px;
  height: 40px;
  
}
.part-box {
  margin-right: 20px;
  vertical-align: middle;
  width: 40px;
  height: 40px;
  border:1rpx solid #e4e4e4;
  border-radius: 6px;
}

.part-text {
  font-size: 12px;
  color: #626574;
  margin-top: 10px;
}
.btm{
  padding: 30px;
  text-align: center;
  color: #888;
}
.red{
  color: red;
}


.swiper {
  height: 345rpx;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}