/* pages/home2/home2.wxss */
/* register.wxss */
/* home.wxss */
.page{
  background: #f8f8f8;
  min-height: 100%;
}
.top {
 
  width: 100%;
  padding: 10px;
}
.quan{
  float: right;
  position: relative;
  top: 4rpx;
}
.quan_a{
  margin-right: 10rpx;
  height: 44rpx;
  width: 44rpx;
  background: #000;
  float: left;
  border-radius: 50%;
} 
.quan_b{
  margin-right: 20rpx;
}
.top .right {
  float: right;
  margin-right: 20px;
}

.icons {
  /* border-top: 1px solid #ccc; */
  height: 80px;
  box-sizing: border-box;
  border-bottom: 1rpx solid #e9e9e9;
  padding: 10px 0;
  width: 100%;
  display: flex;
  flex-direction: row;
  background: white;
  margin-top: -9px;
}

.icon {
  width: 33.33%;
  text-align: center;
  font-size: 14px;
}

.icon image {
  width: 32px;
  height: 32px;
}

.mid{
  padding: 20rpx 20rpx 0 20rpx;
}
.left{
  float: left;
  width: 345rpx;
  height: 248rpx;
  margin-right: 20rpx;
  background-size: 100%;
  background-repeat: no-repeat;
  position: relative;
}
.right_list{
  float: right;
  width: 345rpx;
  height: 248rpx;
}
.right_top{
  width: 345rpx;
  margin-bottom: 20rpx;
  height: 114rpx;
  background-size: 100%;
  background-repeat: no-repeat;
}
.right_bot{
  width: 345rpx;
  height: 114rpx;
  background-size: 100%;
  background-repeat: no-repeat;
}
.star,.content{
  text-indent: 20rpx;
  /* color: #fff; */
}
.content{
  position: absolute;
  bottom: 60rpx;
  font-size: 34rpx;
  font-weight: 500;
}
.star{
  position: absolute;
  bottom: 26rpx;
  font-size: 26rpx;
}
.viprenmai,.zhuanvip{
  text-indent: 20rpx;
}
.viprenmai{
  font-size: 34rpx;
  font-weight: 500;
  padding-top: 5rpx;
}
.zhuanvip{
  font-size: 26rpx;
  color: #808080;
}
.we__title{
 height: 30rpx;
 line-height: 30rpx;
 padding-left:15px;
 padding-right:15px;
 color:#999999;
 font-size:14px;
 padding-top: 20rpx;
 padding-bottom: 20rpx;

}

.weui-cells__title{
  padding: 10px 15px 5px;
}
.weui-cell:before{
  left: 0;
}
.weui-cell{
  height: 80px;
  box-sizing: border-box;
}
.title{
  font-size: 17px;
  color: #2b2d3a;
}
.part {
  margin-right: 20px;
  vertical-align: middle;
  width: 40px;
  height: 40px;
  
}
.part-box {
  margin-right: 20px;
  vertical-align: middle;
  width: 40px;
  height: 40px;
  border:1rpx solid #e4e4e4;
  border-radius: 6px;
}

.part-text {
  font-size: 12px;
  color: #626574;
  margin-top: 10px;
}
.btm{
  padding: 30px;
  text-align: center;
  color: #888;
}
.red{
  color: red;
}


.swiper {
  height: 345rpx;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}

.contact_menu{
  background: white;
  box-sizing: border-box;
  border-top: 1rpx solid #e9e9e9;
  border-bottom: 1rpx solid #e9e9e9;
  overflow: hidden;
  padding: 20rpx 0 0 0;
  margin-top: 20px;
}
.contact_menuitem{
  width: 33.33%;
  float: left;
  text-align: center;
  margin-bottom: 15px;
}
.contact_menuitem image{
  margin: 0 auto; 
}
.cir_img{
  width: 64rpx;
  height: 64rpx;
  display: block;
  padding:18rpx;
  background:#f2f2f2;
  border-radius:50%;
}