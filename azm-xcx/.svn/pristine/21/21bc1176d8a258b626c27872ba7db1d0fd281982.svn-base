.myweui-cell__ft_in-access{
  content: " ";
  display: inline-block;
  height: 6px;
  width: 6px;
  border-width: 0 2px 2px 0;
  border-color: #C8C8CD;
  border-style: solid;
  -webkit-transform: matrix(0.707107,0.707107,-0.707107,0.707107,0,0);
          transform: matrix(0.707107,0.707107,-0.707107,0.707107,0,0);
  position: relative;
  top: -2px;
  top: 50%;
  margin-top: -4px;
  right: 2px;
  margin-right:13px;
}
