<image src="{{imgUrl}}" bindload="imgLoadFinish" hidden="true"></image>
<canvas catchtouchend="touchendImg" catchtouchmove="touchMoveImg" catchtouchstart="touchstartImg"  style="display:block;height:{{cH}}px;width:{{cW}}px;opacity:0.9;" canvas-id="myCanvas3"></canvas>

<view animation="{{animationData2}}" style="position:fixed;top:{{H}}px;left:0px;height:65px;line-height:65px;width:100%;background-color:#F8F8F8;z-index:1000;display:flex;flex-direction:row;">
  <view style="width:100%;display:flex;flex-direction:row;margin-top:12.5px;">
    <button bindtap="openConfirm" style="height:40px;width:145px;background-color:white;font-size:16px;color:#2B2D3A;border:1px solid #2B2D3A;">
      <view style="height:40px;width:55px;margin:auto;display:flex;flex-direction:row;">
        <view>
          <image src="https://media.ccyishe.com/xcx/images/codeExam/reset.png" style="height:16px;width:16px;"></image>
        </view>
        <view style="text-align:left;width:37px;margin-left:2px;">重置</view>
      </view>
    </button>
    <button bindtap="finishToInserTwoCode" style="height:40px;width:130px;background-color:#2B2D3A;font-size:16px;color:white;">
      <view style="height:40px;width:55px;margin:auto;display:flex;flex-direction:row;">
        <view>
          <image src="https://media.ccyishe.com/xcx/images/20170623/picfinish.png" style="height:16px;width:16px;"></image>
        </view>
        <view style="text-align:left;width:37px;margin-left:2px;">完成</view>
      </view>
    </button>
  </view>
</view>